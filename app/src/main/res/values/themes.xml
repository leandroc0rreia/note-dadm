<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Maynote" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#173AE6</item>
        <item name="colorPrimaryDark">#1431BF</item>
        <item name="colorAccent">#FFC107</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.Maynote.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:statusBarColor">#1431BF</item>
    </style>

    <style name="SpinnerTheme" parent="android:Widget.DeviceDefault.Spinner">
        <item name="android:background">@drawable/btnroundgray</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#606060</item>
        <item name="android:fontFamily">@font/ubuntu</item>
    </style>

    <style name="Theme.Maynote.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="Theme.Maynote.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>